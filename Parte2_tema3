public class Main {
    public static void main(String[] args) {
        
        coche miCoche= new coche();
        miCoche.mas_puertas();
        System.out.println(miCoche.puertas);

    }
}
class coche{
    public int puertas = 4;

    public void mas_puertas(){
        this.puertas++;
    }
}
